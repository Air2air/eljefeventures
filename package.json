{
  "name": "eljefeapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "gcp-predeploy": "find ./build -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "find ./build -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "build": "rm -rf ./build && NODE_ENV=production next build",
    "start": "next start -p 8080",
    "deploy": "npm run build && npm run gcp-predeploy && gcloud app deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "bootstrap": "4.5.3",
    "graphql": "^15.5.1",
    "next": "11.0.1",
    "node-sass": "^4.10.0",
    "react": "17.0.2",
    "react-bootstrap": "^1.4.3",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.9",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "typescript": "^4.3.5"
  }
}
