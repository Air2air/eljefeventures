{
  "name": "eljefeapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "gcp-predeploy": "find ./build -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "find ./build -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "build": "rm -rf ./build && NODE_ENV=production next build",
    "start": "next start -p 8080",
    "deploy": "npm run build && npm run gcp-predeploy && gcloud app deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "framer-motion": "^4.1.17",
    "isomorphic-unfetch": "^3.1.0",
    "mongodb": "^3.6.8",
    "mongoose": "^5.13.2",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "devDependencies": {
    "@nivo/bar": "^0.72.0",
    "@nivo/core": "^0.72.0",
    "@nivo/pie": "^0.72.0",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.9",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "sass": "^1.35.2",
    "typescript": "^4.3.5"
  }
}
